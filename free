<?php
//$f=parse_ini_file('/etc/amportal.conf',true);
$f=file('/etc/amportal.conf');

//print_r( $f);
foreach($f as $k=>$s){
	//echo $c=count($s);
	if(strlen($s)>1){
		if($s[0]!='#'){
			//echo $k.' = '.$s.'<br>';
			$vv=explode("=",$s);
		
			//echo $vv[0].'='.$vv[1].'<br>';
				//$amportal[$vv[0]]=str_replace(' ','',$vv[1]);
				$amportal[$vv[0]]=trim($vv[1]);
			
			
		}
		//echo $s;
	}
}
//print_r($amportal);
var_dump($amportal);
?>